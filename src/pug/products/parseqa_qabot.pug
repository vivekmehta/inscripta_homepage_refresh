                .row.justify-content-center
                    .col-6
                        img.img-fluid.lazyload(src="/assets/img/products/parseqa-qabot/key-features-1.png")
                    .col-6
                        img.img-fluid.lazyload(src="/assets/img/products/parseqa-qabot/key-features-2.png")



        // Demo Video
        section#parseqa-qabot-demo.page-section.bg-light
            .container.px-4.px-lg-5
                .row.gx-4.gx-lg-5.align-items-center
                    .col-lg-8.col-md-12
                        .responsive-video-wrapper(style="position: relative; padding-bottom: 56.25%; height: 0; overflow: hidden; border-radius: 12px;")
                            video#parseqa-qabot-demo-video(
